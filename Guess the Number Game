#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main() {
int ran_val;
srand(time(0));
rand_val = rand() % 100;
int user;
do {
printf("===========Welcome to 'GUESS THE NUMBER GAME"===============\n Enter the your no. : ");
scanf("%d, &user);
if(user == rand_val){
printf("congrats you guessed correct number !!1");
}
else if(user > rand_val){
printf("guessed no > random no. !!");
}
else if(user < rand_val){
printf("guessed no < random no. !!");
}
else{
printf("congrats you guessed wrong number! \n please try again.....");
}
} while(user != rand_value);
return 0;
}
